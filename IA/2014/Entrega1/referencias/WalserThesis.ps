/D    85 def

/eule {				% D eule
  /D exch def			% Durchmesser
  /u D 110 div def

  gsave
  currentpoint translate
  u u scale

  /CirctextDict 16 dict def

  /mt	{moveto}	def
  /lt	{lineto}	def
  /ct	{curveto}	def
  /np	{newpath}	def
  /cp	{closepath}	def
  /slw	{setlinewidth}	def
  /Weiss	{1 setgray}	def
  /Schwarz	{0 setgray}	def

  /R1	55	def		% Aussen-Radius
  /R2	38	def		% Innen-Radius
  /EyeX	  5.0	def
  /EyeY  19.0	def
  /EyeR0 13.0	def
  /EyeR1  6.2	def
  /EyeR2  4.3	def
  /EyeR3  3.0	def
  /EyeR4  2.4	def

  2 slw
  np 0 0 R1 0 360 arc Schwarz cp stroke
  np 0 0 R2 0 360 arc Schwarz cp stroke

  /OutsideCircleText {
    CirctextDict begin		% Vier Argumente:
      /Radius exch def		% Radius des Kreises (0,0) als Mittelpunkt
      /Centerangle exch def	% Winkelposition zum Zentrieren des Textes
      /Str exch def		% der zu plazierende Text
      gsave
      Centerangle Str
      FindHalfangle add rotate
      Str
      { /Charcode exch def
        ( ) dup 0 Charcode put OutsideplaceChar
      } forall
      grestore
    end
  } def

  /InsideCircleText {
    CirctextDict begin		% Argumente wie OutsideCircleText
      /Radius exch def		%
      /Centerangle exch def
      /Str exch def
      gsave
      Centerangle Str FindHalfangle sub rotate
      Str
      { /Charcode exch def
        ( ) dup 0 Charcode put InsideplaceChar
      } forall
      grestore
    end
  } def

  CirctextDict begin
    /FindHalfangle {
      stringwidth pop 2 div
      2 XRadius mul
      Pi mul div 360 mul
    } def
  
    /OutsideplaceChar {
      /Char exch def
      /Halfangle
      Char FindHalfangle def
      gsave
      Halfangle neg rotate
      Radius 0 translate
      -90 rotate
      Char stringwidth pop
      2 div neg 0 mt
      Char show
      grestore
      Halfangle 2 mul neg rotate
    } def
  
    /InsideplaceChar {
      /Char exch def
      /Halfangle
      Char FindHalfangle def
      gsave
      Halfangle rotate
      Radius 0 translate
      90 rotate
      Char stringwidth pop
      2 div neg 0 mt
      Char show
      grestore
      Halfangle 2 mul rotate
    } def

    /Pi	3.1415923 def
  end

  Schwarz
%%BeginFont: Helvetica-Bold
  /Helvetica-Bold findfont 12 scalefont setfont
  /Rmidth  46.5 def	% {R1 R2 add 2 div}
  /XRadius 32.1 def	% {Rmidth 12 1.2 mul sub}
  (UNIVERSITAS)  90 Rmidth 4 sub OutsideCircleText
  (SARAVIENSIS) 270 Rmidth 4 add InsideCircleText
%%EndFonts

  /HalfEule {
	np
	0 25 5 25 9 33 7 33 3 27.5 0 27.5 mt lt lt lt lt lt
	Schwarz cp fill
	np
	9 33 14 -24 16 -24 11 33 mt lt lt lt
	Schwarz cp fill
	np
	4 -27 20 -27 21 -28 20 -24 5 -24 2 -28 mt lt lt lt lt lt
	Schwarz cp fill
	np
	10 -19 19 -28 19 -31 7 -19 mt lt lt lt
	Schwarz cp fill
	
	np
	0 -19 mt
	EyeX EyeY EyeR1
	EyeR1 EyeR1 mul EyeX EyeX mul sub sqrt EyeX atan 180 add 360 arc
	14 -19 lt
	Schwarz cp fill

	np
	0 12.5 1 12.5 0 11 mt lt lt
	Weiss cp fill
	np
	11 33 27 25 13 19 12.5 16 27 15 20 -23 26.5 -21 27 -21 14 -15 mt ct lt ct lt
	Schwarz cp fill
	np
	24.5 1 14 -16 22 -18 24.5 -18 14 -8 14 1 mt lt ct lt
	Weiss cp fill
	
	1 slw
	np EyeX EyeY EyeR1 -45 45 arc Weiss cp fill
	np EyeX EyeY EyeR1 0 360 arc Schwarz cp stroke
	np EyeX EyeY EyeR2 0 360 arc Schwarz cp fill

	1.8 slw
	[54 90 126 162 198 270 342] {
	  /alpha exch def
	  np
	  alpha sin EyeR2 mul EyeX add alpha cos EyeR2 mul EyeY add mt
	  alpha sin EyeR1 mul EyeX add alpha cos EyeR1 mul EyeY add lt
	  Schwarz cp stroke
	} forall

	np
	18 sin EyeR2 mul EyeX add 18 cos EyeR2 mul EyeY add mt
	18 sin EyeR0 mul EyeX add 18 cos EyeR0 mul EyeY add lt
	Schwarz cp stroke

	np EyeX EyeY EyeR3 0 360 arc Weiss cp fill
	np EyeX EyeY EyeR4 0 360 arc Schwarz cp fill

	0.8 slw
	/feder {
	  /FY exch def
	  /FX exch def
	  np
	  FX 0.85 sub FY mt
	  FX 0.85 sub FY 3 sub lt
	  FX FY 3 sub 0.85 180 360 arc
	  FX 0.85 add FY lt
	  stroke
	} def

	Weiss 
	0 1 2 {
	  3.75 mul /k exch def
	  k 10 feder
	  1.875 k add 4.5 feder
	  1.875 k add -6.5 feder
	  15 k add 12 feder
	  16.875 k add 17.5 feder
	} for
	0 1 3 {
	  3.75 mul /k exch def
	  k -1 feder
	  k -12 feder
	  13.125 k add 6.5 feder
	} for

	Schwarz
	0 1 1 {
	  /k exch def
	  /POS k 3.5 mul 17.5 add def
	  np
	  POS -18 k sub POS 1 mt lt
	  cp stroke
	} for
	0 1 2 {
	  3.5 mul 15.5 add /k exch def
	  np
	  k -4.5 k 1 mt lt
	  cp stroke
	} for

	1.5 slw np 
	14.5 -24 mt
	EyeX EyeR1 add EyeY 2 sub lt
	Schwarz stroke
	2.2 slw np
	27 24 13 18 12.5 15 25.8 15 25.8 0
	mt lt ct
	Schwarz stroke
  } def
  
  HalfEule
  -1 1 scale
  HalfEule

  grestore
} def

LLx D 2 div add LLy D 2 div add moveto D 1 sub eule
